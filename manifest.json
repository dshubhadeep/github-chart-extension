{
  "manifest_version": 2,
  "name": "Github Chart Extension",
  "version": "1.0.0",
  "author": "Shubhadeep Das",
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["storage", "tabs", "webRequest", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
